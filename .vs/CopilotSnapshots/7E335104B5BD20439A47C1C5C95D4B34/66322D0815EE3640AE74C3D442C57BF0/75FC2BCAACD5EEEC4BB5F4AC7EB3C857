using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Otob_sBilet.R.S
{
    public class Koltuk
    {
        public int Numara { get; set; }
        public bool DoluMu { get; set; }

        public Koltuk(int numara)
        {
            Numara = numara;
            DoluMu = false;
        }
    }
}
